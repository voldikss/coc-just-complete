{
  "name": "coc-just-complete",
  "version": "0.0.2",
  "description": "One kind of buffer words completion",
  "main": "lib/index.js",
  "engines": {
    "coc": "^0.0.79"
  },
  "keywords": [
    "coc.nvim",
    "completion"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build",
    "lint": "eslint . --ext .ts"
  },
  "activationEvents": [
    "*"
  ],
  "repository": "https://github.com/voldikss/coc-just-complete",
  "homepage": "https://github.com/voldikss/coc-just-complete/#readme",
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "just-complete.shortcut": {
          "type": "string",
          "default": "just-complete"
        },
        "just-complete.priority": {
          "type": "number",
          "default": 3
        },
        "just-complete.ignoreGitignore": {
          "type": "boolean",
          "default": true,
          "description": "Ignore git ignored files for gathering buffer words"
        }
      }
    }
  },
  "author": "dyzplus@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "@voldikss/tsconfig": "*",
    "coc.nvim": "^0.0.80",
    "eslint": "^7.16.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0"
  }
}
